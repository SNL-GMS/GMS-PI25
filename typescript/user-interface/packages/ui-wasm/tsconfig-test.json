{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./.cache/.tsbuildinfo-test",
    "paths": {
      "@gms/common-model": ["../common-model/src/ts/common-model.ts"],
      "@gms/common-model/lib/*": ["../common-model/src/ts/*"],
      "@gms/common-util": ["../common-util/src/ts/common-util.ts"],
      "@gms/common-util/lib/*": ["../common-util/src/ts/*"],
      "@gms/ui-util": ["../ui-util/src/ts/ui-util.ts"],
      "@gms/ui-util/lib/*": ["../ui-util/src/ts/*"],
      "@gms/weavess-core": ["../weavess-core/src/ts/weavess-core.ts"],
      "@gms/weavess-core/lib/*": ["../weavess-core/src/ts/*"]
    }
  },
  "references": [
    {
      "path": "../common-util/tsconfig-test.json"
    },
    {
      "path": "../ui-util/tsconfig-test.json"
    },
    {
      "path": "../weavess-core/tsconfig-test.json"
    }
  ],
  "include": [
    "src/**/*",
    "src/ts/**/*.mjs",
    "__tests__/**/*",
    "__mocks__/**/*",
    "__tests__/gms-filters-provider/gms-filters/90-minute-waveform-payload.json",
    "__tests__/gms-filters-provider/gms-filters/validation/data-in.json",
    "__tests__/gms-filters-provider/gms-filters/validation/data-out-1.json",
    "__tests__/gms-filters-provider/gms-filters/validation/data-out-2.json",
    "__tests__/gms-filters-provider/gms-filters/validation/data-out-3.json",
    "__tests__/gms-filters-provider/gms-filters/validation/data-out-4.json",
    "__tests__/gms-filters-provider/gms-filters/validation/data-out-5.json",
    "__tests__/gms-filters-provider/gms-filters/validation/data-out-6.json"
  ]
}
